{
	"name": "webpack-typescript-starter",
	"version": "1.0.0",
	"description": "A simple starter template for SPA with Webpack and TypeScript",
	"private": true,
	"scripts": {
		"start:dev": "cross-env NODE_ENV=development webpack-dev-server --open",
		"build:dev": "cross-env NODE_ENV=development webpack --mode development",
		"build:prod": "cross-env NODE_ENV=production webpack --mode production",
		"prettier:write": "prettier --write .",
		"prettier:check": "prettier --check .",
		"lint": "eslint .",
		"lint:fix": "eslint --fix ."
	},
	"keywords": [],
	"author": "d-indifference (https://github.com/d-indifference)",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/d-indifference/webpack-typescript-starter"
	},
	"devDependencies": {
		"@types/node": "^17.0.0",
		"@typescript-eslint/eslint-plugin": "^5.7.0",
		"@typescript-eslint/parser": "^5.7.0",
		"babel-eslint": "^10.1.0",
		"clean-webpack-plugin": "^4.0.0",
		"copy-webpack-plugin": "^10.2.0",
		"cross-env": "^7.0.3",
		"css-loader": "^6.5.1",
		"eslint": "^8.5.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-webpack-plugin": "^3.1.1",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^5.5.0",
		"mini-css-extract-plugin": "^2.5.3",
		"node-sass": "^7.0.1",
		"prettier": "^2.5.1",
		"resolve-url-loader": "^5.0.0",
		"sass-loader": "^12.4.0",
		"scss-loader": "^0.0.1",
		"style-loader": "^3.3.1",
		"ts-loader": "^9.2.6",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.4",
		"webpack": "^5.65.0",
		"webpack-cli": "^4.9.1",
		"webpack-dev-server": "^4.6.0"
	}
}
